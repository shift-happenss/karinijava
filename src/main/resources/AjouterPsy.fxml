<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f4f4f4;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="consultation.psy.controllers.AjouterPsyController">
   <AnchorPane layoutX="30.0" layoutY="20.0" />

                                                
                                                    <Button layoutX="152.0" layoutY="349.0" onAction="#ajouterPsy" text="Ajouter" />
    <Button layoutX="370.0" layoutY="349.0" onAction="#afficherPsy" text="Afficher" />
    <Button fx:id="btnRetour" layoutX="520.0" layoutY="14.0" onAction="#retour" text="← Retour" />
   <Group layoutX="152.0" layoutY="52.0">
      <children>
      
                                                                      <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Ajouter un psy" />
      
                                                                      <Label layoutY="50.0" text="Nom :" />
          <TextField fx:id="nomTF" layoutX="120.0" layoutY="45.0" />
      
                                                                      <Label layoutY="90.0" text="Numéro Tel :" />
          <TextField fx:id="numTF" layoutX="120.0" layoutY="85.0" />
      
                                                                      <Label layoutY="130.0" text="Email :" />
          <TextField fx:id="emailTF" layoutX="120.0" layoutY="125.0" />
      
                                                                      <Label layoutY="170.0" text="Spécialité :" />
          <TextField fx:id="specTF" layoutX="120.0" layoutY="165.0" />
      
                                                                  
                                                                      <Label layoutY="200.0" text="Date :" />
          <DatePicker fx:id="dateTF" layoutX="120.0" layoutY="195.0" />
      
                                                                      <Label layoutY="240.0" text="Heure :" />
          <TextField fx:id="heureTF" layoutX="120.0" layoutY="235.0" promptText="HH:mm" />
      </children>
   </Group>

                                                </AnchorPane>
